/* components/myservice/myservice.wxss */
.container {
  display: flex;
  width: 92%;
  height: 100rpx;
  border-radius: 20rpx;
  background-color: #ffffff;
  margin: 0 auto 140rpx;
}
.box1 {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
}
.wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 200rpx;
  height: 60rpx;
}
.img {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  background-color: #fafafa;
}
.wrapper-img {
  width: 28rpx;
  height: 28rpx;
}
.txt {
  font-size: 30rpx;
  font-weight: bolder;
  font-family: serif;
}
.border {
  position: relative;
}
.border::after {
  position: absolute;
  content: '';
  height: 50%;
  width: 100%;
  border-right: 1rpx solid #dedede;
}